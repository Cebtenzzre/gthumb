	<owner>gthumb</owner>
	<type>bool</type>
	<default>true</default>
	<locale name="C">
	  <short></short>
	  <long></long>
	</locale>
      </schema>

      <schema>
	<key>/schemas/apps/gthumb/ext/resize_images/allow_swap_width_height</key>
	<applyto>/apps/gthumb/ext/resize_images/allow_swap_width_height</applyto>
	<owner>gthumb</owner>
	<type>bool</type>
	<default>true</default>
	<locale name="C">
	  <short></short>
	  <long></long>
	</locale>
      </schema>

      <schema>
	<key>/schemas/apps/gthumb/ext/resize_images/mime_type</key>
	<applyto>/apps/gthumb/ext/resize_images/mime_type</applyto>
	<owner>gthumb</owner>
	<type>string</type>
	<default></default>
	<locale name="C">
	  <short></short>
	  <long></long>
	</locale>
      </schema>

    </schemalist>
</gconfschemafile>
