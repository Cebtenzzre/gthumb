	<applyto>/apps/gthumb/browser/thumbnail_size</applyto>
	<owner>gthumb</owner>
	<type>int</type>
	<default>128</default>
	<locale name="C">
	  <short></short>
	  <long>
	  </long>
	</locale>
      </schema>

      <schema>
	<key>/schemas/apps/gthumb/browser/thumbnail_limit</key>
	<applyto>/apps/gthumb/browser/thumbnail_limit</applyto>
	<owner>gthumb</owner>
	<type>int</type>
	<default>0</default>
	<locale name="C">
