      </schema>

      <schema>
	<key>/schemas/apps/gthumb/browser/fast_file_type</key>
	<applyto>/apps/gthumb/browser/fast_file_type</applyto>
	<owner>gthumb</owner>
	<type>bool</type>
	<default>true</default>
	<locale name="C">
	  <short></short>
