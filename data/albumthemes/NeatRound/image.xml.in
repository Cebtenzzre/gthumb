<html>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
<meta name="Generator" content="gThumb Image Viewer">
<head>
    <gthumb:if images > 1 />
      <title><gthumb:header/> (<gthumb:image_idx/>/<gthumb:images/>)</title>
    <gthumb:else/>
      <title><gthumb:header/></title>
    <gthumb:end/>
    <style type="text/css">
        a:link { 
		font-weight: bold; 
		text-decoration: none;
		color: #969696;      
	}
	a:visited { 
		font-weight: bold; 
		text-decoration: none;
		color: #969696;
	}
        a:hover { 
		font-weight: bold; 
		text-decoration: none; 
		color: gold;
	}
	span.note {
		color: gray;
		font-size: x-small;
	}
	span.copyright {
		color: gray;
		font-size: x-small;
		text-align: center;
	}
    </style>
</head>
<body bgcolor="#ffffff">
	<!-- Navigation Bar -->

	<table border=0 cellspacing=0 width="100%">
          <tr>
	    <td colspan=4 align="center" width="100%">
	      <font color="#555555" size=+2>
	        <b><gthumb:header/></b>
	      </font>
	    </td>
	  </tr>
	  <tr>
	    <td align="left" width="88%">
	      <a href="<gthumb:page_link idx_relative=0 image_idx/>">
		<img src="back.png" width="30" height="30" border="0"></a>
	    </td>
            <gthumb:if images == 1 />
	      <td align="right" width="1%">
		&nbsp;
	      </td>
	      <td align="right" width="1%">
		&nbsp;
	      </td>
	      <td align="right" width="1%">
		&nbsp;
	      </td>
            <gthumb:end/>
            <gthumb:if images > 1 />
	      <td align="right" width="1%">
                <gthumb:if image_idx == 1 />
		  <img src="background.gif" width="40" height="30" border="0"></td>
	        <gthumb:end/>
                <gthumb:if image_idx > 1 />
		  <a href="<gthumb:image_link idx_relative=-1/>">
		    <img src="prev.png" align="center" width="40" height="30" border="0"></a> </td>
	        <gthumb:end/>
	      <td valign="center" align="center" width="10%">
		<font style=bold color="#969696">
		<b><gthumb:image_idx/> / <gthumb:images/></b>
		</font>
	      </td>
	      <td align="right" width="1%">
                <gthumb:if image_idx == images />
		  <img src="background.gif" width="40" height="30" border="0">
	        <gthumb:end/>
                <gthumb:if image_idx < images />
		  <a href="<gthumb:image_link idx_relative=+1/>">
		    <img src="next.png" align="center" width="40" height="30" border="0"> </a>
	        <gthumb:end/>
	      </td>
            <gthumb:end/>
	  </tr>
	</table>

	&nbsp;<BR>

        <table width="100%" border="0" align="center">
          <tr align="left">

            <gthumb:if image_idx == 1 />
	      <td>
		<img src="background.gif" width="100" height="100" border="0"></td>
	    <gthumb:end/>

            <gthumb:if image_idx > 1 />	
	      <td>
	        <a href="<gthumb:image_link idx_relative="-1"/>">
	          <gthumb:image thumbnail max_size=100 idx_relative="-1" border="2"/></a>
	      </td>
	    <gthumb:end/>

            <td align="center" colspan="2">
              <a href="<gthumb:file_name with_relative_path/>"><gthumb:image preview border=2/></a><BR>
            </td>

            <gthumb:if image_idx == images />
	      <td>
		<img src="background.gif" width="100" height="100" border="0">
	      </td>
	    <gthumb:end/>

            <gthumb:if image_idx < images />	
    	      <td align="right">
	        <a href="<gthumb:image_link idx_relative="+1"/>">
		  <gthumb:image thumbnail max_size=100 idx_relative="+1" border="2"/></a>
	      </td>
	    <gthumb:end/>

          </tr>
          <tr align="left">
	    <td>&nbsp;</td>
            <td align="right" colspan="2">
              <span class=note>
                <_gthumb:text>(click on the image to view it at original size)</_gthumb:text>
              </span>
	    </td>
	    <td>&nbsp;</td>
	  </tr>
          <tr>
            <td colspan="4">
              &nbsp;
	    </td>
          </tr>
          <tr align="left">
	    <td>&nbsp;</td>
            <td colspan="2">
	      <font color="black">
                <gthumb:if comment_visibility_image/>
		  <gthumb:comment/>
                <gthumb:end/>
	      </font>
	    </td>
	    <td>&nbsp;</td>
          </tr>
          <tr>
            <td colspan="4">
              &nbsp;
	    </td>
          </tr>
          <gthumb:if file_name_visibility_image/>
            <tr align="left">
	    <td>&nbsp;</td>
            <td>
              <font color="#969696"><nobr><b><_gthumb:text>Image</_gthumb:text></b></nobr></font>
	    </td>
	    <td width="100%">
      	      <font color="black">
	        <tt><gthumb:file_name/></tt>
	      </font>
	    </td>
	    <td>&nbsp;</td>
            </tr>
          <gthumb:end/>
          <gthumb:if image_dim_visibility_image/>
            <tr align="left">
	    <td>&nbsp;</td>
            <td>
              <font color="#969696"><nobr><b><_gthumb:text>Dimensions</_gthumb:text></b></nobr></font>
	    </td>
	    <td width="100%">
	      <font color="black">
		 <gthumb:image_dim/> pixels
	      </font>
	    </td>
	    <td>&nbsp;</td>
            </tr>
          <gthumb:end/>
          <gthumb:if file_size_visibility_image/>
            <tr align="left">
	    <td>&nbsp;</td>
            <td>
              <font color="#969696"><nobr><b><_gthumb:text>Bytes</_gthumb:text></b></nobr></font>
	    </td>
	    <td width="100%">
	      <font color="black">
		 <gthumb:file_size/>
	      </font>
	    </td>
	    <td>&nbsp;</td>
            </tr>
          <gthumb:end/>
          <gthumb:if exif_date_time_visibility_image/>
            <tr align="left">
	    <td>&nbsp;</td>
            <td>
              <font color="#969696"><nobr><b><_gthumb:text>Date</_gthumb:text></b></nobr></font>
	    </td>
	    <td width="100%">
	      <font color="black">
		 <gthumb:exif:date_time/>
	      </font>
	    </td>
	    <td>&nbsp;</td>
            </tr>
          <gthumb:end/>
          <gthumb:if exif_exposure_time_visibility_image/>
            <tr align="left">
	    <td>&nbsp;</td>
            <td>
              <font color="#969696"><nobr><b><_gthumb:text>Exposure time</_gthumb:text></b></nobr></font>
	    </td>
	    <td width="100%">
	      <font color="black">
		 <gthumb:exif:exposure_time/>
	      </font>
	    </td>
	    <td>&nbsp;</td>
            </tr>
          <gthumb:end/>
          <gthumb:if exif_exposure_mode_visibility_image/>
            <tr align="left">
	    <td>&nbsp;</td>
            <td>
              <font color="#969696"><nobr><b><_gthumb:text>Exposure mode</_gthumb:text></b></nobr></font>
	    </td>
	    <td width="100%">
	      <font color="black">
		 <gthumb:exif:exposure_mode/>
	      </font>
	    </td>
	    <td>&nbsp;</td>
            </tr>
          <gthumb:end/>
          <gthumb:if exif_flash_visibility_image/>
            <tr align="left">
	    <td>&nbsp;</td>
            <td>
              <font color="#969696"><nobr><b><_gthumb:text>Flash</_gthumb:text></b></nobr></font>
	    </td>
	    <td width="100%">
	      <font color="black">
		 <gthumb:exif:flash/>
	      </font>
	    </td>
	    <td>&nbsp;</td>
            </tr>
          <gthumb:end/>
          <gthumb:if exif_shutter_speed_visibility_image/>
            <tr align="left">
	    <td>&nbsp;</td>
            <td>
              <font color="#969696"><nobr><b><_gthumb:text>Shutter speed</_gthumb:text></b></nobr></font>
	    </td>
	    <td width="100%">
	      <font color="black">
		 <gthumb:exif:shutter_speed/>
	      </font>
	    </td>
	    <td>&nbsp;</td>
            </tr>
          <gthumb:end/>
          <gthumb:if exif_aperture_value_visibility_image/>
            <tr align="left">
	    <td>&nbsp;</td>
            <td>
              <font color="#969696"><nobr><b><_gthumb:text>Aperture value</_gthumb:text></b></nobr></font>
	    </td>
	    <td width="100%">
	      <font color="black">
		 <gthumb:exif:aperture_value/>
	      </font>
	    </td>
	    <td>&nbsp;</td>
            </tr>
          <gthumb:end/>
          <gthumb:if exif_focal_length_visibility_image/>
            <tr align="left">
	    <td>&nbsp;</td>
            <td>
              <font color="#969696"><nobr><b><_gthumb:text>Focal length</_gthumb:text></b></nobr></font>
	    </td>
	    <td width="100%">
	      <font color="black">
		 <gthumb:exif:focal_length/>
	      </font>
	    </td>
	    <td>&nbsp;</td>
            </tr>
          <gthumb:end/>
          <gthumb:if exif_camera_model_visibility_image/>
            <tr align="left">
	    <td>&nbsp;</td>
            <td>
              <font color="#969696"><nobr><b><_gthumb:text>Camera model</_gthumb:text></b></nobr></font>
	    </td>
	    <td width="100%">
	      <font color="black">
		 <gthumb:exif:camera_model/>
	      </font>
	    </td>
	    <td>&nbsp;</td>
            </tr>
          <gthumb:end/>
        </table>

	&nbsp;<BR>
	&nbsp;<BR>

	<!-- Navigation Bar -->

	<table align="center" border=0 cellspacing=0 width="100%">
	  <tr>
	    <td bgcolor="#969696">
	      <img src="1.gif" height="1" border="0"></td></tr>
	  <tr>
	    <td align="center">
              <span class=copyright><gthumb:footer/></span></td></tr>
	</table>
</body>
</html>
