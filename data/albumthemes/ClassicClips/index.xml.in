<gthumb:set_var thumbnail_width=159 thumbnail_height=120/>
<gthumb:set_var preview_width=400 preview_height=400/>

<html>
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
  <meta name="Generator" content="gThumb Image Viewer">
  <head>
    <gthumb:if pages > 1 />
      <title><gthumb:header/> (<gthumb:page_idx/>/<gthumb:pages/>)</title>
    <gthumb:else/>
      <title><gthumb:header/></title>
    <gthumb:end/>
    <style type="text/css">
        a:link { 
		font-weight: bold; 
		text-decoration: none;
		color: white       
	}
	a:visited { 
		font-weight: bold; 
		text-decoration: none;
		color: white
	}
        a:hover { 
		font-weight: bold; 
		text-decoration: none; 
		color: gold 
	}
	span.copyright {
		color: white;
		font-size: small;
		text-align: center;
	}
    </style>
  </head>
  <body bgcolor="#4b4b4b">
	<!-- Navigation Bar -->

	<table border=0 cellspacing=0 width="100%">
	  <tr>
            <td width="83%">
            </td>
	    <gthumb:if pages == 1 />
	      <td align="left" width="1%">
	        &nbsp;
	      </td>
	      <td align="left" width="1%">
	        &nbsp;
	      </td>
	      <td align="left" width="1%">
	        &nbsp;
	      </td>
	    <gthumb:else/>
	      <td align="left" width="1%">
	        <gthumb:if page_idx == 1 />
		  <img src="background.gif" width="40" height="30" border="0"></td>
		<gthumb:else_if page_idx > 1 />
	          <a href="<gthumb:page_link idx_relative=-1/>">
		    <img src="prev.png" align="center" width="40" height="30" border="0"></a></td>
	        <gthumb:end/>
	      <td valign="center" align="center" width="10%">
		<font style=bold color="white">
		  <b><gthumb:page_idx/> / <gthumb:pages/></b>
		</font>
	      </td>
	      <td align="left" width="1%">
		<gthumb:if page_idx == pages />
		  <img src="background.gif" width="40" height="30" border="0"></td>
		<gthumb:else_if page_idx < pages />
		  <a href="<gthumb:page_link idx_relative=+1/>">
		    <img src="next.png" align="center" width="40" height="30" border="0"></a></td>
		<gthumb:end/>
	    <gthumb:end/>
          <tr>
	    <td colspan=4 align="center" width="100%">
	      <font color="#FFFFFF" size=+2>
	        <b><gthumb:header/></b>
	      </font>
	    </td>
	  </tr>
	</table>

	&nbsp;<BR>

	<table bgcolor="#4b4b4b" border="0" align="center">
	<gthumb:table/>
	</table>

	&nbsp;<BR>
	&nbsp;<BR>
	
	<!-- Navigation Bar -->

	<table align=center border=0 cellspacing=0 width="90%">
	  <tr>
	    <td bgcolor="#969696">
	      <img src="1.gif" height="1" border="0"></td></tr>
	  <tr>
	    <td align="center">
	      &nbsp;<BR>
              <span class=copyright><gthumb:footer/></span></td></tr>
	</table>
  </body>
</html>
